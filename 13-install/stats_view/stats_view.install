<?php

/**
 * @file
 * Install and uninstall scripts for Stats View module.
 * @package stats_view
 */

/**
 * Implements hook_uninstall().
 */
function stats_view_uninstall() {
  // Remove all variables declared by stats_view module.
  foreach (node_type_get_types() as $type) {
    variable_del('stats_view_'. $type->type);
  }
  variable_del('stats_view_max_entries');
}